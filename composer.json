{
    "name": "larapackages/repository",
    "type": "library",
    "description": "Laravel Repository Pattern",
    "require": {
        "php": "^7.3",
        "illuminate/database": "^8.0",
        "laravel/legacy-factories": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Larapackages\\Repository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larapackages\\Tests\\": "tests/"
        }
    }
}
